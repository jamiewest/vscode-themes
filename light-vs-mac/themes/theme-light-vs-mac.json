{
    "$schema": "vscode://schemas/color-theme",
    "name": "Light (Visual Studio for Mac)",
    "include": "./light_defaults.json",
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#888a85"
            }
        },
        // {
        //     "name": "Comment Doc",
        //     "scope": [
        //         "entity.name.tag.localname.charp",
        //         "entity.name.tag.charp",
        //         "meta.tag.charp",
        //         "comment.block.documentation.charp",
        //         "source"
        //     ],
        //     "settings": {
        //         "foreground": "#888a85"
        //     }
        // },
        {
            "name": "css tags in selectors, xml tags",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#3465a4"
            }
        },
        {
            "scope": "entity.name.selector",
            "settings": {
                "foreground": "#800000"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#222222"
            }
        },
        // {
        //     "scope": "constant",
        //     "settings": {
        //         "foreground": "#db7100"
        //     }
        // },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#009695"
            }
        },
        {
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#db7100"
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#e5ae41"
            }
        },
        {
            "scope": "entity",
            "settings": {
                "foreground": "#3465a4"
            }
        },
        {
            "name": "function",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "name": "namespace",
            "scope": "entity.name.type.namespace",
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "name": "variable (local)",
            "scope": "entity.name.variable.local",
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "name": "variable",
            "scope": "entity.name.variable",
            "settings": {
                "foreground": "#888a85"
            }
        },
        {
            "name": "parameter",
            "scope": "entity.name.variable.parameter",
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "name": "property",
            "scope": "entity.name.variable.property",
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#c44d58"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#009695"
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "foreground": "#009695"
            }
        },
        {
            "scope":"keyword.operator",
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.cast",
                "keyword.operator.sizeof",
                "keyword.operator.logical.python"
            ],
            "settings": {
                "foreground": "#3465a4"
            }
        },
        {
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#009695"
            }
        },
        {
            "scope": "markup",
            "settings": {
                "foreground": ""
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#000080"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#75507b"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#4e9a06"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#ef2929"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#3465a4"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#800000"
            }
        },
        {
            "scope": "meta.selector",
            "settings": {
                "foreground": "#800000"
            }
        },
        {
            "scope": "meta.preprocessor",
            "settings": {
                "foreground": "#3465a4"
            }
        },
        {
            "scope": "meta.preprocessor.string",
            "settings": {
                "foreground": "#a31515"
            }
        },
        {
            "scope": "meta.preprocessor.numeric",
            "settings": {
                "foreground": "#009695"
            }
        },
        {
            "scope": "meta.structure.dictionary.key.python",
            "settings": {
                "foreground": "#0451a5"
            }
        },
        {
            "scope": "meta.diff.header",
            "settings": {
                "foreground": "#4e9a06",
                "font-style": "bold"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#009695"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#3465a4"
            }
        },
        {
            "scope": "storage.modifier",
            "settings": {
                "foreground": "#3465a4"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#db7100"
            }
        },
        {
            "scope": [
                "string.comment.buffered.block.jade",
                "string.quoted.jade",
                "string.interpolated.jade",
                "string.unquoted.plain.in.yaml",
                "string.unquoted.plain.out.yaml",
                "string.unquoted.block.yaml",
                "string.quoted.single.yaml",
                "string.quoted.double.xml",
                "string.quoted.single.xml",
                "string.unquoted.cdata.xml",
                "string.quoted.double.html",
                "string.quoted.single.html",
                "string.unquoted.html",
                "string.quoted.single.handlebars",
                "string.quoted.double.handlebars"
            ],
            "settings": {
                "foreground": "#db7100"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#811f3f"
            }
        },
        {
            "scope": "support",
            "settings": {
                "foreground": "#db7100"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "name": "brackets of XML/HTML tags",
            "scope": "punctuation.definition.tag",
            "settings": {
                "foreground": "#800000"
            }
        },
        {
            "scope": "support.type.property-name.json",
            "settings": {
                "foreground": "#3465a4"
            }
        },
        {
            "name": "json property boolean",
            "scope": [
                "constant.language",
                "meta.structure.dictionary.value",
                "meta.structure.dictionary",
                "meta.structure.array",
                "source"
            ],
            "settings": {
                "foreground": "#009695"
            }
        },
        {
            "name": "json property brackets",
            "scope": [
                "punctuation.separator.dictionary.pair",
                "punctuation.definition.dictionary",
                "meta.structure.dictionary.value",
                "meta.structure.dictionary",
                "punctuation.definition.array",
                "meta.structure.array",
                "source"
            ],
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "name": "brackets of XML/HTML tags",
            "scope": "punctuation.definition.tag",
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "scope": [
                "punctuation.section.embedded.begin.metatag.php",
                "punctuation.section.embedded.end.metatag.php"
            ],
            "settings": {
                "foreground": "#800000"
            }
        },
        {
            "scope": "support.function.git-rebase",
            "settings": {
                "foreground": "#0451a5"
            }
        },
        {
            "scope": "constant.sha.git-rebase",
            "settings": {
                "foreground": "#fed9c0"
            }
        },
        {
            "name": "coloring of the Java import and package identifiers",
            "scope": [
                "storage.modifier.import.java",
                "storage.modifier.package.java"
            ],
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "name": "this.self",
            "scope": "variable.language",
            "settings": {
                "foreground": "#009695"
            }
        }

    ]
}